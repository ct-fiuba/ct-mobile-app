{
  "expo": {
    "name": "Contact Tracing",
    "slug": "contact-tracing-fiuba",
    "owner": "ct-fiuba",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#02734B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fiuba.contact-tracing"
    },
    "android": {
      "package": "com.fiuba.contact_tracing"
    }
  }
}
